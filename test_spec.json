{
  "id": 0,
  "datadir": "dir",
  "logdir": "/tmp/gennn/",
  "validate_each_n_steps": 100,
  "max_number_of_iterations": 200,
  "max_runtime": 10,
  "hyperparameters":
  {
    "learningrate": 1e-4,
    "optimizer": "AdamOptimizer",
    "batchsize": 50
  },
  "layers":
  [
    {
      "type": "conv_layer",
      "convolution": {
        "filter": {
          "height": 5,
          "width": 5,
          "inchannels": 1,
          "outchannels": 16
        },
        "strides": {
          "x": 1,
          "y": 1,
          "inchannels": 1,
          "batch": 1
        }

      },
      "n_featuremaps": 5,
      "activation_function": "relu"
    },
    {
      "type": "maxpool_layer",
      "kernel":
      {
        "height": 2,
        "width": 2,
        "inchannels": 1,
        "outchannels": 1
      },
      "strides":
      {
        "y": 2,
        "x": 2,
        "inchannels": 1,
        "batch": 1
      }
    },
    {
      "type": "feedforward_layer",
      "size": 1024,
      "activation_function": "relu"
    }

  ]

}